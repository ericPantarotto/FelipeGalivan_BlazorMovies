@page "/"
@page "/index"
@using BlazorMovies.Client.Helpers;
@inject IRepository repository

<div>
    <h3>Movie</h3>
    <MoviesList Movies="movies" />
</div>

@code {
    private List<Movie>? movies = new();

    protected override void OnInitialized()
    {
        movies = repository.GetMovies();
    }


} 